{
  "name": "veleserp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=develop ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=develop tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=testing jest",
    "test:watch": "jest --watch",
    "test:cov": "cross-env NODE_ENV=testing jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.5.3",
    "dotenv": "^8.1.0",
    "mongoose": "^5.6.10",
    "mongoose-paginate-v2": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.18",
    "@types/mongoose": "^5.5.13",
    "@types/mongoose-paginate-v2": "^1.0.3",
    "@types/node": "12.7.2",
    "@types/supertest": "2.0.8",
    "cross-env": "^5.2.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsc-watch": "2.4.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3"
  }
}