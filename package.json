{
  "name": "veleserp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=develop ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=develop tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=develop jest",
    "test:watch": "jest --watch",
    "test:cov": "cross-env NODE_ENV=develop jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.7.2",
    "class-validator": "^0.10.0",
    "cron": "^1.7.2",
    "docxtemplater": "^3.14.3",
    "dotenv": "^8.1.0",
    "mongoose": "^5.7.1",
    "pizzip": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.7.2",
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.18",
    "@types/mongoose": "^5.5.17",
    "@types/node": "12.7.5",
    "@types/supertest": "2.0.8",
    "cross-env": "^6.0.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "typescript": "3.6.3"
  }
}
